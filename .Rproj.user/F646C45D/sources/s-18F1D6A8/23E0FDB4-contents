




# Create a function to pool results from ordinal random effects models.
library(ordinal)
library(mice)


fmm1 <- clmm(rating ~ temp + contact + (1|judge), data = wine)

dotplot(ranef(fmm1, condVar = T))
imp <- mice(wine)

res <- with(imp, clmm(rating ~ temp + contact + (1|judge), data = wine))

pool(res)





  
plot(pooled_est)
ggplot(pooled_est$random_effects, aes(x = mode, y = factor(cluster))) + geom_point() + geom_errorbarh(pooled_est$random_effects, aes(xmax = mode + 2 * sqrt(cond_var))